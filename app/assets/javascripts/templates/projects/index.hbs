<h1>All Projects</h1>

<table>
  <tr>
    <th>Project Name</th>
  </tr>
  {{#each project in controller}}
    <tr>
      <td>{{project.name}}</td>
      <td>{{#link-to 'projects.show' project}}View{{/link-to}}</td>
      <td>{{#link-to 'projects.edit' project}}Edit{{/link-to}}</td>
    </tr>
  {{/each}}
</table>

